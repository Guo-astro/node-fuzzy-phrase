{
  "name": "node-fuzzy-phrase",
  "version": "0.1.0",
  "description": "neon module for wrapping fuzzy-phrase",
  "main": "lib/index.js",
  "author": "Andrea del Rio <adelrio@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "neon-cli": "^0.1.22",
    "node-pre-gyp": "^0.10.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.243.1",
    "tape": "4.6.3"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build=false || neon build",
    "build": "neon build",
    "test": "tape test/*.js"
  },
  "binary": {
    "module_name": "index",
    "host": "https://mapbox-node-binary.s3.amazonaws.com",
    "remote_path": "./{name}/v{version}/{configuration}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native/"
  }
}
